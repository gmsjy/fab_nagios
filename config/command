# The following examples allow user-supplied arguments and can
# only be used if the NRPE daemon was compiled with support for
# command arguments *AND* the dont_blame_nrpe directive in this
# config file is set to '1'.  This poses a potential security risk, so
# make sure you read the SECURITY file before doing this.

command[check_users]=/opt/nagios/nagios//libexec/check_users -w $ARG1$ -c $ARG2$
command[check_load]=/opt/nagios/nagios//libexec/check_load -w $ARG1$ -c $ARG2$
command[check_disk]=/opt/nagios/nagios//libexec/check_disk -w $ARG1$ -c $ARG2$ -p $ARG3$
command[check_procs]=/opt/nagios/nagios//libexec/check_procs -w $ARG1$ -c $ARG2$ -s $ARG3$

